[Term]
id: NPO:1717
name: x-ray wave
is_a: NPO:1714 ! electromagnetic wave
relationship: part_of NPO:1734 ! x-ray radiation
property_value: code NPO:1717 xsd:string
property_value: definition "<ncicp:ComplexDefinition xmlns:ncicp=\"http://ncicb.nci.nih.gov/xml/owl/EVS/ComplexProperties.xsd#\"><ncicp:def-definition>An electromagnetic wave which has wavelengths between 10 nm and 0.001 nm, frequencies between 30 PHz and 100 EHz (1 PHz - 10^15 Hz, 1 EHz = 10^18 Hz), and energies between 124 eV and 1.24 MeV. An X-ray wave has wavelength shorter than that of an ultraviolet wave and longer than that of a gamma-ray wave</ncicp:def-definition><ncicp:Definition_Review_Date>100603</ncicp:Definition_Review_Date><ncicp:def-source>NPO</ncicp:def-source><ncicp:attr>http://hyperphysics.phy-astr.gsu.edu/hbase/ems3.html#c4 (June 3, 2010)</ncicp:attr><ncicp:Definition_Reviewer_Name>Dennis Thomas</ncicp:Definition_Reviewer_Name></ncicp:ComplexDefinition>" xsd:string
property_value: FULL_SYN "<ncicp:ComplexTerm xmlns:ncicp=\"http://ncicb.nci.nih.gov/xml/owl/EVS/ComplexProperties.xsd#\"><ncicp:term-name>x-ray wave</ncicp:term-name><ncicp:term-group>PT</ncicp:term-group><ncicp:term-source>NCI</ncicp:term-source></ncicp:ComplexTerm>" xsd:string
property_value: preferred_Name "x-ray wave" xsd:string

[Term]
id: NPO:1718
name: ultraviolet wave
is_a: NPO:1714 ! electromagnetic wave
relationship: part_of NPO:1735 ! ultraviolet radiation
relationship: realizes NPO:1755 ! ultraviolet light energy
property_value: code NPO:1718 xsd:string
property_value: definition "<ncicp:ComplexDefinition xmlns:ncicp=\"http://ncicb.nci.nih.gov/xml/owl/EVS/ComplexProperties.xsd#\"><ncicp:def-definition>An electromagnetic wave which has wavelengths between 400 nm and 10 nm, frequencies between 300 THz and 750 THz (1 THz = 10^12 Hz), and energies between 3.1 eV and 124 eV.  An ultraviolet wave has a wavelength shorter than that of a visible light wave, but longer than that of an x-ray wave.</ncicp:def-definition><ncicp:Definition_Review_Date>100603</ncicp:Definition_Review_Date><ncicp:def-source>NPO</ncicp:def-source><ncicp:attr>http://hyperphysics.phy-astr.gsu.edu/hbase/ems3.html#c4 (June 3, 2010)</ncicp:attr><ncicp:Definition_Reviewer_Name>Dennis Thomas</ncicp:Definition_Reviewer_Name></ncicp:ComplexDefinition>" xsd:string
property_value: FULL_SYN "<ncicp:ComplexTerm xmlns:ncicp=\"http://ncicb.nci.nih.gov/xml/owl/EVS/ComplexProperties.xsd#\"><ncicp:term-name>ultraviolet wave</ncicp:term-name><ncicp:term-group>PT</ncicp:term-group><ncicp:term-source>NCI</ncicp:term-source></ncicp:ComplexTerm>" xsd:string
property_value: preferred_Name "ultraviolet wave" xsd:string
property_value: synonym "UV wave" xsd:string

[Term]
